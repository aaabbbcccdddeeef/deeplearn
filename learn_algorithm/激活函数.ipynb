    "import numpy as np;\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + np.exp(-x))\n",
    "\n",
    "x = np.array([1, 2, 3])\n",
    "print(sigmoid(x))\n",
    "# 输出：[0.73105858 0.88079708 0.95257413]\n",
